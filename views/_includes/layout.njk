        {% set css %}
        {% include "kraken.css" %}
        {% include "custom.css" %}
        {% endset %}

        <style>
            {{css | cssmin | safe}}
        </style>
    </head>

    <body class="type-page page-single">

        <div class="container container-large">

            {# <a class="screen-reader screen-reader-focusable" href="#main">Skip to main content</a> #}

            {# <a class="screen-reader screen-reader-focusable"
            href="mailto:branjwong@hey.com?subject=Diino%20Does:%20Accessibility%20Feedback">Accessibility
            Feedback</a> #}

        </div>

        <header class="container container-large margin-bottom-large">
            <nav class="nav-wrap">
                <a class="logo" href="/">
                    <svg xmlns="http://www.w3.org/2000/svg" height="0.8em" width="0.8em" viewBox="0 0 512 512"><!--!Font Awesome Free 6.5.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.--><path d="M256 64c53 0 96 43 96 96s-43 96-96 96s-96 43-96 96s43 96 96 96C150 448 64 362 64 256S150 64 256 64zm0 448A256 256 0 1 0 256 0a256 256 0 1 0 0 512zm32-352a32 32 0 1 0 -64 0 32 32 0 1 0 64 0zM224 352a32 32 0 1 1 64 0 32 32 0 1 1 -64 0z"/></svg>
                Diino Does
            </a>
                <div class="nav-menu" id="nav-menu">
                    <ul class="nav">
                        <li>
                            <a {{ 'aria-current="page"' | safe if '/about' in page.url }}  href="/about">About</a>
                        </li>
                        <li>
                            <a {{ 'aria-current="page"' | safe if '/web' in page.url  }} href="/web">Web</a>
                        </li>
                        <li>
                            <a {{ 'aria-current="page"' | safe if '/tutoring' in page.url }} href="/tutoring">Tutoring</a>
                        </li>
                        <li>
                            <a {{ 'aria-current="page"' | safe if '/consulting' in page.url }} href="/consulting">Consulting</a>
                        </li>
                    </ul>

                </div>
            </nav>
        </header>

        <main id="main" tabindex="-1">

            <div class="container">
                {{ content | safe }}
            </div>
        </main>

        <footer class="container container-large">
            <hr class="margin-bottom">
            <nav class="tabindex nav-secondary" id="nav-secondary" tabindex="-1">
                <ul class="list-inline list-inline-responsive">
                    <li>
                        <a href="/about">About</a>
                    </li>
                    <li>
                        <a href="/web">Web</a>
                    </li>
                    <li>
                        <a href="/tutoring">Tutoring</a>
                    </li>
                    <li>
                        <a href="/consulting">Consulting</a>
                    </li>
                </ul>
            </nav>
            <p class="text-small">Made with ❤️ in Vancouver. Unless otherwise noted, all code is free to use under the <a href="/mit">MIT License</a>.
            </p>
        </footer>

        <script>
            /* ! dd v0.0.0 | (c) 2024 Brandon Wong | MIT License | http://github.com/branjwong/diion-does | Credits: http://github.com/cferdinandi/go-make-things */
            !function () {
                "use strict";
                navigator && navigator.serviceWorker && navigator
                    .serviceWorker
                    .register("/sw.js"),
                navigator.serviceWorker.controller && window.addEventListener("load", (function () {
                    navigator
                        .serviceWorker
                        .controller
                        .postMessage("cleanUp")
                }))
            }();
        </script>
    </body>

</html>